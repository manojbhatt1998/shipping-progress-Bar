<script>
    window.bundleComponents = 4;
    window.bundleComponentsSelected = [];
    window.bundleCost = 0;

    document.querySelectorAll('.bundle-quantities .bundle-quantity-box').forEach(quantity => {
        quantity.addEventListener('click' , function(e) {
            if(!this.classList.contains('selected')) {
                document.querySelector('.bundle-quantities .bundle-quantity-box.selected').classList.remove('selected');
                this.classList.add('selected');
                var thisQuantity = this;
                document.querySelectorAll('.bundle-selection').forEach(selection => {
                  selection.dataset.bundleComponents = thisQuantity.dataset.bundleComponents;
                });
                document.querySelectorAll('.bundle-component-quantity-selector input').forEach(quantityInput => {
                  quantityInput.max = 4;
                });
                window.bundleComponents = parseInt(this.dataset.bundleComponents);
                bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
            }
        });
    });

    document.querySelectorAll('.bundle-component-select').forEach(component => {
        component.addEventListener('click', function(e) {
            e.preventDefault();
            if(window.bundleComponentsSelected.length < 4) {
                if(window.bundleComponents == window.bundleComponentsSelected.length) {
                  window.bundleComponents++;
                  document.querySelector('.bundle-quantity-box[data-bundle-components="'+window.bundleComponents+'"]').click();
                }
                window.bundleComponentsSelected.push(parseInt(this.dataset.variantId));
                window.bundleCost += parseInt(this.dataset.variantPrice);
                this.parentElement.classList.add('selected');
                this.parentElement.querySelector('input').value = 1;                           
            }
            bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
        });
    });

    document.querySelectorAll('.bundle-component-quantity-selector button').forEach(componentQuantityButton => {
      componentQuantityButton.addEventListener('click', function(e) {
        e.preventDefault();
        var currentQuantityInput = this.parentElement.querySelector('input');
        var component = this.closest('.bundle-component-actions');
        var componentData = component.querySelector('.bundle-component-select');
        if(this.classList.contains('bundle-minus')) {          
          if(currentQuantityInput.value <= 1) {
            currentQuantityInput.value = 0;
            component.classList.remove('selected');
          } else {
            currentQuantityInput.value = parseInt(currentQuantityInput.value)-1;
          }
          var indexOfVariantInBundle = window.bundleComponentsSelected.lastIndexOf(parseInt(componentData.dataset.variantId));
          window.bundleComponentsSelected.splice(indexOfVariantInBundle,1);
          window.bundleCost -= parseInt(componentData.dataset.variantPrice);
          bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
        } else {
          if(window.bundleComponentsSelected.length < 4) {
            if(window.bundleComponents == window.bundleComponentsSelected.length) {
              window.bundleComponents++;
              document.querySelector('.bundle-quantity-box[data-bundle-components="'+window.bundleComponents+'"]').click();
            }
            console.log(currentQuantityInput);
            currentQuantityInput.value = parseInt(currentQuantityInput.value)+1;
            window.bundleComponentsSelected.push(componentData.dataset.variantId);
            window.bundleCost += parseInt(componentData.dataset.variantPrice);            
          }
          bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
        }
      })
    });

    document.querySelectorAll('.bundle-component-remove').forEach(componentQuantityRemove => {
      componentQuantityRemove.addEventListener('click', function(e) {
        e.preventDefault();
        var currentQuantityInput = this.parentElement.querySelector('input');
        var component = this.closest('.bundle-component-actions');
        var componentData = component.querySelector('.bundle-component-select');
        if(currentQuantityInput.value > 0) {
          while(window.bundleComponentsSelected.lastIndexOf(parseInt(componentData.dataset.variantId)) !== -1) {
            var indexOfVariantInBundle = window.bundleComponentsSelected.lastIndexOf(parseInt(componentData.dataset.variantId));
            window.bundleComponentsSelected.splice(indexOfVariantInBundle,1);
          }
          window.bundleCost -= parseInt(componentData.dataset.variantPrice) * currentQuantityInput.value;
          bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
        }
        currentQuantityInput.value = 0;
        component.classList.remove('selected');
      })
    });

    //Disable writing component quantity value manually
    document.querySelectorAll('.bundle-component-quantity-selector input').forEach(componentQuantityInput => {
      componentQuantityInput.addEventListener('keypress', function(e) {
        e.preventDefault();
      })
    });

    /* If bundle is set */
    var queryString = window.location.search;
    var urlParams = new URLSearchParams(queryString);
    var removeKeys = [];
    var currentBundleComponents = 4;
    if(urlParams.get('bundle-id')) {
      var currentBundleId = urlParams.get('bundle-id');
      fetch(window.Shopify.routes.root + 'cart.js', {
        method: 'GET',
        headers: {
          'Content-Type': 'application/json'
        }
      })
      .then(response => {
        return response.json();
      })
      .then(data => {
        data.items.forEach(cartItem => {
          if(cartItem.properties._bundle_id && cartItem.properties._bundle_id == currentBundleId) {
            removeKeys.push(cartItem.key);
            currentBundleComponents = cartItem.properties._bundle_components;
            var bundleComponent = document.querySelector('.bundle-component-select[data-variant-id="'+cartItem.id+'"]');
            var bundleComponentActions = bundleComponent.parentElement;
            var bundleComponentValue = bundleComponentActions.querySelector('input');
            if(!bundleComponentActions.classList.contains('selected')) {
              bundleComponentActions.classList.add('selected');
            }
            bundleComponentValue.value = parseInt(bundleComponentValue.value) + 1;
            window.bundleCost += parseInt(cartItem.price);
            window.bundleComponentsSelected.push(cartItem.id);
          }
        });
        window.bundleComponents = currentBundleComponents;
        if(currentBundleComponents != 4) {
          document.querySelector('.bundle-quantities .bundle-quantity-box.selected').classList.remove('selected');
          document.querySelectorAll('.bundle-quantities .bundle-quantity-box').forEach(quantity => {
            if(quantity.dataset.bundleComponents == currentBundleComponents) {
              quantity.classList.add('selected')
            }
          });
          document.querySelectorAll('.bundle-selection').forEach(selection => {
            selection.dataset.bundleComponents = currentBundleComponents;
          });
          document.querySelectorAll('.bundle-component-quantity-selector input').forEach(quantityInput => {
            quantityInput.max = 4;
          });
        }    
            
        bundleBuildSelection(window.bundleComponentsSelected, window.bundleComponents);
        document.querySelector('.bundle-collection').classList.add('loaded');
      })
      .catch((error) => {
        console.error('Error:', error);
        document.querySelector('.bundle-collection').classList.add('loaded');
      });      
    } else {
      document.querySelector('.bundle-collection').classList.add('loaded');
    }

    //!!!! ADD TO CART !!!!
document.querySelectorAll('.bundle-add-to-cart').forEach(atc => {
  atc.addEventListener('click', function(e) {
    e.preventDefault();

    if (!atc.getAttribute('disabled')) {
      const _bundle_id = Date.now();
      const formData = { items: [] };

      window.bundleComponentsSelected.forEach((component, index) => {
        const variantId = parseInt(component);
        if (isNaN(variantId)) return; // Skip invalid IDs

        const itemData = {
          id: variantId,
          quantity: 1,
          properties: {
            _bundle_id: String(_bundle_id),
            _bundle_components: window.bundleComponentsSelected.length.toString(),
            _bundle_component_position: index.toString(),
            _bundle_url: 'Bundle'
          }
        };

        if (urlParams.get('bundle-id') && removeKeys.length) {
          itemData.properties._remove_bundle_id = urlParams.get('bundle-id');
          removeKeys = [];
        }

        formData.items.push(itemData);
      });

      fetch(`${window.Shopify.routes.root}cart/add.js`, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
          'X-Requested-With': 'XMLHttpRequest'
        },
        body: JSON.stringify(formData)
      })
        .then(response => response.json())
        .then(response => {
          const cartDrawer = document.querySelector('cart-drawer') || document.querySelector('cart-notification');

          // If cart drawer is present, refresh its content
          if (cartDrawer && typeof cartDrawer.renderContents === 'function') {
            cartDrawer.renderContents(response);
          } else {
            // Fallback to redirect if cart drawer isn't available
            window.location = window.Shopify.routes.cart_url;
          }
        })
        .catch(error => {
          console.error('Error:', error);
        });
    }
  });
});

    document.querySelector('.bundle-pick-products').addEventListener('click', function(e) {
      e.preventDefault();
     // location.hash = "#ProductGridContainer";
      const targetDiv = document.getElementById('ProductGridContainer');
      if (targetDiv) {
        targetDiv.scrollIntoView({ behavior: 'smooth', block: 'start' });
      }
    });

    document.querySelector('.bundle-go-up').addEventListener('click', function(e) {
      e.preventDefault();
      location.hash = "#bundle-collection-summary";
    });

    function bundleBuildSelection(array, total) {
      var bundleBoxes = document.querySelectorAll('.bundle-selection.bundle-selection-normal .bundle-selection-box');
      var bundleBoxesSticky = document.querySelectorAll('.bundle-selection.bundle-selection-sticky .bundle-selection-box');
      if(window.bundleComponentsSelected.length < 4) {
        document.querySelectorAll('.bundle-component-quantity-selector .bundle-plus').forEach(plusQuantity => {
          plusQuantity.classList.remove('disabled');
        }); 
        document.querySelectorAll('.bundle-limit').forEach(bundleLimit => {
          bundleLimit.classList.remove('show');
        });
      } else {
        document.querySelectorAll('.bundle-component-quantity-selector .bundle-plus').forEach(plusQuantity => {
          plusQuantity.classList.add('disabled');
        });
        document.querySelectorAll('.bundle-limit').forEach(bundleLimit => {
          bundleLimit.classList.add('show');
        });
      }
      var componentsToRemove = [];
      for(var i = 1; i <= 4; i++) {
        if(array[i-1]) {
            bundleBoxes[i-1].innerHTML = `<img src="`+document.querySelector('.bundle-component-select[data-variant-id="'+array[i-1]+'"]').dataset.variantImage+`" alt="">`;
            bundleBoxesSticky[i-1].innerHTML = `<img src="`+document.querySelector('.bundle-component-select[data-variant-id="'+array[i-1]+'"]').dataset.variantImage+`" alt="">`;
        } else {
            bundleBoxes[i-1].innerHTML = `<span class="icon-bundle-bottle"></span>`;
            bundleBoxesSticky[i-1].innerHTML = `<span class="icon-bundle-bottle"></span>`;
        }

        if(i > total && array[i-1]) {
          componentsToRemove.push(array[i-1]);              
        }            
      }

      if(componentsToRemove.length) {
        for(var j = 0; j<componentsToRemove.length; j++) {
          var variantIdToRemove = componentsToRemove[j]; 
          var selectedQuantity = document.querySelector('input[name="bundle-component['+variantIdToRemove+']"]');
          if(selectedQuantity.value == 1) {
            selectedQuantity.closest('.bundle-component-actions').classList.remove('selected');
          }
          selectedQuantity.value = parseInt(selectedQuantity.value) - 1;
          window.bundleCost -= parseInt(document.querySelector('.bundle-component-select[data-variant-id="'+variantIdToRemove+'"]').dataset.variantPrice);
          var indexOfVariantInBundle = window.bundleComponentsSelected.lastIndexOf(parseInt(variantIdToRemove));
          window.bundleComponentsSelected.splice(indexOfVariantInBundle,1);
        }          
      }

      bundleTotalUpdate(window.bundleCost, window.bundleComponentsSelected);

      /* enable/disable add to cart buttons */
      if(array.length == total) {
        document.querySelectorAll('.bundle-add-to-cart[disabled]').forEach(atc => {
          atc.removeAttribute('disabled');
        });        
      } else {
        document.querySelectorAll('.bundle-add-to-cart:not([disabled])').forEach(atc => {
          atc.setAttribute('disabled',true);
        });
      }
    }

    function bundleTotalUpdate(cost, array) {
        var bundleSubtotal = document.querySelector('.bundle-collection-summary .bundle-subtotal');
        var bundleSubtotalFooter = document.querySelector('.bundle-footer-fixed .bundle-subtotal');
        var bundleSubtotalCost = document.querySelector('.bundle-collection-summary .bundle-subtotal-cost');
        var bundleSubtotalCostFooter = document.querySelector('.bundle-footer-fixed .bundle-subtotal-cost');
        var bundleDiscounts = {
            2: 10,
            3: 15,
            4: 20
        }
        var bundleDiscountCost = cost - cost * (bundleDiscounts[array.length]/100);

        bundleSubtotal.innerText = "Subtotal ("+array.length+" item"+(array.length == 1 ? "": "s")+")";
        bundleSubtotalFooter.innerText = "Subtotal ("+array.length+" item"+(array.length == 1 ? "": "s")+")";
        if(array.length>1) {
          bundleSubtotalCost.innerHTML = "<span class='original-cost'>$"+(cost/100).toFixed(2)+"</span> $"+(bundleDiscountCost/100).toFixed(2); 
          bundleSubtotalCostFooter.innerHTML = "<span class='original-cost'>$"+(cost/100).toFixed(2)+"</span> $"+(bundleDiscountCost/100).toFixed(2); 
        } else {
          bundleSubtotalCost.innerHTML = "$"+(cost/100).toFixed(2); 
          bundleSubtotalCostFooter.innerHTML = "$"+(cost/100).toFixed(2); 
        }
    }
</script>


{% schema %}
